  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-openssl": "*",
    "tourze/workerman-psr-logger": "0.0.*",
    "workerman/workerman": "^5.1"
  },
  "autoload": {
    "psr-4": {
